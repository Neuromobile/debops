{% set ghostunnel__tpl_etc_services = [] %}
{% for server in ghostunnel__combined_servers %}
{%   set _ = ghostunnel__tpl_etc_services.append({
   'name': server.name,
   'port': server.listen.split(":")[1]|string
}) %}
{% endfor %}
